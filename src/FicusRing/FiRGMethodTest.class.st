	#name : #FiRGMethodTest,
	#superclass : #FiRGShotTest,
	#category : #'FicusRing-Tests'
}

{ #category : #tests }
FiRGMethodTest >> testMessages [
		
	self deny: ((ringShotResource classA methods collect: #messages) anySatisfy: #isNil)
]

{ #category : #tests }
FiRGMethodTest >> testParseTree [

	self deny: ((ringShotResource classA methods collect: #parseTree) anySatisfy: #isNil).

]

{ #category : #tests }
FiRGMethodTest >> testProtocol [
		
	self flag: #todo. "Missing more cases"
	self assert: (ringShotResource classA >> #sum) protocol equals: #autogenerated.

]

{ #category : #tests }
FiRGMethodTest >> testRefersToLiteral [
		
	self assert: ((ringShotResource classA >> #sum42) refersToLiteral: #sum)
]
